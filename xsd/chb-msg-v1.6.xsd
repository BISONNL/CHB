		<xs:annotation>
			<xs:documentation>abri aanwezig?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="shelterpublicity">
		<xs:annotation>
			<xs:documentation>reclame in abri?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="illuminatedstop">
		<xs:annotation>
			<xs:documentation>Halte verlicht?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="seatavailable">
		<xs:annotation>
			<xs:documentation>zitgelegenheid bij halte?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="leantosupport">
		<xs:annotation>
			<xs:documentation>stasteun aanwezig bij halte?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="timetableinformation">
		<xs:annotation>
			<xs:documentation>haltevertrekstaat aanwezig?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="iconuri">
		<xs:annotation>
			<xs:documentation>Absolute URI naar de publiek toegankelijke locatie waarvandaan de afbeelding geladen kan worden. De extensie van de file geeft het soort afbeelding aan. Ondersteunde bestandsoorten zijn gif, jpg, jpeg, png, svg, webp.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:anyURI">
				<xs:maxLength value="1024"/>
				<xs:pattern value="[hH][tT][tT][pP][sS]?://.+"/>
				<!-- http/https lower and uppercase and combinations like hTTpS:// -->
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="infounit">
		<xs:annotation>
			<xs:documentation>vitrine voor haltevertrekstaat/statische reisinformatie aanwezig?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="routenetworkmap">
		<xs:annotation>
			<xs:documentation>lijnennetkaart aanwezig?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="passengerinformationdisplay">
		<xs:annotation>
			<xs:documentation>actuele reisinformatie aanwezig (DRIS)?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="passengerinformationdisplaytype">
		<xs:annotation>
			<xs:documentation>type DRIS paneel</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="45"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="audiobutton">
		<xs:annotation>
			<xs:documentation>Audioknop aanwezig?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="bicycleparking">
		<xs:annotation>
			<xs:documentation>Fietsparkeervoorziening aanwezig?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="numberofbicycleplaces">
		<xs:annotation>
			<xs:documentation>Aantal Fietsparkeerplaatsen</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:unsignedShort">
				<xs:minInclusive value="0"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="bins">
		<xs:annotation>
			<xs:documentation>Afvalbakken aanwezig?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ovccico">
		<xs:annotation>
			<xs:documentation>OV chipcard in/uit-check faciliteit aanwezig?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ovccharging">
		<xs:annotation>
			<xs:documentation>OV chipcard oplaadfaciliteiten aanwezig?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="rd-x">
		<xs:annotation>
			<xs:documentation>RD-X, coördinaat mag max. + of - 3 meter afwijken van werkelijke positie.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:minInclusive value="-7000"/>
				<xs:maxInclusive value="300000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="rd-y">
		<xs:annotation>
			<xs:documentation>RD-Y, coördinaat mag max. + of - 3 meter afwijken van werkelijke positie.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:minInclusive value="289000"/>
				<xs:maxInclusive value="629000"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="rd-z">
		<xs:simpleType>
			<xs:restriction base="xs:int">
				<xs:minInclusive value="-50"/>
				<xs:maxInclusive value="500"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="street">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="100"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="town">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="100"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="location">
		<xs:annotation>
			<xs:documentation>Nadere aanduiding locatie halte</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="1"/>
				<xs:maxLength value="100"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="quayname">
		<xs:annotation>
			<xs:documentation>Naam halte voor publiek</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="100"/>
				<xs:minLength value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="stopsidecode">
		<xs:annotation>
			<xs:documentation>Perronletter/aanduiding</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="quayownercode">
		<xs:annotation>
			<xs:documentation>DataOwnercode van type weg/infrabeheerder (type ROOW)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10"/>
				<xs:minLength value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="quayimagedate">
		<xs:annotation>
			<xs:documentation>Opnamedatum van foto van quay</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:dateTime">
				<xs:minInclusive value="1990-01-01T00:00:00Z"/>
				<xs:pattern value="\p{Nd}{4}-\p{Nd}{2}-\p{Nd}{2}T\p{Nd}{2}:\p{Nd}{2}:\p{Nd}{2}Z"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="quayimageurl">
		<xs:annotation>
			<xs:documentation>URL naar foto van quay</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:anyURI">
				<xs:maxLength value="1024"/>
				<xs:pattern value="[hH][tT][tT][pP][sS]?://.+"/>
				<!-- http/https lower and uppercase and combinations like hTTpS:// -->
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="stopplaceimagedate">
		<xs:annotation>
			<xs:documentation>Opnamedatum van foto van stopplace</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:dateTime">
				<xs:minInclusive value="1990-01-01T00:00:00Z"/>
				<xs:pattern value="\p{Nd}{4}-\p{Nd}{2}-\p{Nd}{2}T\p{Nd}{2}:\p{Nd}{2}:\p{Nd}{2}Z"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="stopplaceimageurl">
		<xs:annotation>
			<xs:documentation>URL naar foto van stopplace</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:anyURI">
				<xs:maxLength value="1024"/>
				<xs:pattern value="[hH][tT][tT][pP][sS]?://.+"/>
				<!-- http/https lower and uppercase and combinations like hTTpS:// -->
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="stopobjectcode">
		<xs:annotation>
			<xs:documentation>haltenummer op haltebord (voorzover aanwezig)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="stopinternalcode">
		<xs:annotation>
			<xs:documentation>intern nummer beheerder </xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="stopinternalname">
		<xs:annotation>
			<xs:documentation>interne naam quay voor beheerder</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="100"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="onlygetout">
		<xs:annotation>
			<xs:documentation>wordt halte alleen gebruikt als uitstaphalte?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="quaytype">
		<xs:annotation>
			<xs:documentation>type haltegebruik, enumeration: calamity,season,regular,temporary,demandresponsive</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="calamity"/>
				<xs:enumeration value="season"/>
				<xs:enumeration value="regular"/>
				<xs:enumeration value="temporary"/>
				<xs:enumeration value="demandresponsive"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="environmentinfo">
		<xs:annotation>
			<xs:documentation>informatie (bijv. plattegrond) over omgeving aanwezig?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="toiletfacility">
		<xs:annotation>
			<xs:documentation>toiletvoorziening aanwezig?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="ptbikerental">
		<xs:annotation>
			<xs:documentation>OV-fietsverhuur aanwezig?</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:boolean"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="publicnamemedium">
		<xs:annotation>
			<xs:documentation>Medium naam, altijd incl. plaatsnaam, waaronder knooppunt/station bekend is bij het publiek.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="100"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="publicnamelong">
		<xs:annotation>
			<xs:documentation>Haltenaam incl. nadere eenduidige aanduiding locatie, bijv. kruising van wegen.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="stopplaceindication">
		<xs:annotation>
			<xs:documentation>Aanduiding van StopPlace voor gebruik in openbare ruimte tbv wayfinding</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="stopplacestatus">
		<xs:annotation>
			<xs:documentation>Status van stopplace. enum: [adapted|available|expired]. ‘adapted' betekent dat de StopPlaceStatus door de afnemer alleen kan worden bepaald uit de Quay statussen van de Quays die behoren tot de StopPlace.</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="adapted"/>
				<xs:enumeration value="available"/>
				<xs:enumeration value="expired"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="description">
		<xs:annotation>
			<xs:documentation>Nadere omschrijving</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="quayimagedescription">
		<xs:annotation>
			<xs:documentation>Nadere omschrijving quay foto, hier kan worden aangegeven vanuit welke positie de foto is genomen</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="stopplaceimagedescription">
		<xs:annotation>
			<xs:documentation>Nadere omschrijving stopplace foto, hier kan worden aangegeven vanuit welke positie de foto is genomen</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="255"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="remarks">
		<xs:annotation>
			<xs:documentation>Opmerkingen/bijzonderheden</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="remarkstatus">
		<xs:annotation>
			<xs:documentation>Status van opmerkingen/bijzonderheden, enum: geldig, vervallen</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="valid"/>
				<xs:enumeration value="expired"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="quaystatus">
		<xs:annotation>
			<xs:documentation>Status van quay. enum: [plan|available|expired|unavailable|outofuse|deleted].</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="plan"/>
				<xs:enumeration value="available"/>
				<xs:enumeration value="expired"/>
				<xs:enumeration value="unavailable"/>
				<xs:enumeration value="outofuse"/>
				<xs:enumeration value="deleted"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="concessionprovidercode">
		<xs:annotation>
			<xs:documentation>DataOwnercode van type OV autoriteit (COPR)</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="10"/>
				<xs:minLength value="1"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
</xs:schema>
